extends ../templates/base

append config
	-currentPage = 5

block main
	section.acm-cover
		h1.title Proyectos del capitulo
	form.acm-inlineform
		input(type="text" name="activity")#activity
		section.acm-form_inlineChoices
			article.acm-form_choise
				input(type="radio" name="type" value="all" checked)#all
				label(for="all") Todos
			article.acm-form_choise
				input(type="radio" name="type" value="unfinished")#unfinished
				label(for="unfinished") Sin finalizar
			article.acm-form_choise
				input(type="radio" name="type" value="finisehd")#finisehd
				label(for="finisehd") Finalizados
		button.btn.btn_active Buscar
